<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ebetracking.appv4" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>EBE TRACKING V4</name>
    <description>
        Herramienta para reducir el uso de papel.
    </description>
    <author email="ingjeanpaulrojas@gmail.com.com" href="http://ingjprojas.zz.com.ve">
        Ing. Jean Paul Iván Rojas Véliz
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="fullscreen" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
